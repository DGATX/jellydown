# Server Configuration
PORT=6942
NODE_ENV=development

# Session secret - generate a random string for production
SESSION_SECRET=change-this-to-a-random-secret

# Temp directory for download segments
TEMP_DIR=./temp

# Download settings
MAX_CONCURRENT_SEGMENTS=3
MAX_RETRIES=5
SEGMENT_TIMEOUT_MS=30000

# Session max age (24 hours in milliseconds)
SESSION_MAX_AGE_MS=86400000
