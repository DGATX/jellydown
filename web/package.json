{
  "name": "jellyfin-web-downloader",
  "version": "1.0.0",
  "description": "Download transcoded videos from Jellyfin servers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node server/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:mobile": "playwright test client/__tests__/mobile.spec.ts",
    "test:mobile:headed": "playwright test client/__tests__/mobile.spec.ts --headed",
    "test:mobile:ui": "playwright test client/__tests__/mobile.spec.ts --ui"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "session-file-store": "^1.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.0",
    "@types/session-file-store": "^1.2.6",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^4.0.17",
    "memfs": "^4.52.0",
    "nock": "^14.0.10",
    "playwright": "^1.57.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "vitest": "^4.0.17"
  }
}
